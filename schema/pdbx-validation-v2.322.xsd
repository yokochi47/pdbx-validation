   &lt;/PDBxv:pdbx_validation_software&gt;
   &lt;PDBxv:pdbx_validation_software ordinal="4"&gt;
      &lt;PDBxv:classification&gt;phasing,refinement&lt;/PDBxv:classification&gt;
      &lt;PDBxv:details xsi:nil="true" /&gt;
      &lt;PDBxv:name&gt;Xtriage&lt;/PDBxv:name&gt;
