                        </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="provider" minOccurs="0" maxOccurs="1" nillable="true">
              <xsd:annotation>
                <xsd:documentation xml:lang="en" source="http://mmcif.wwpdb.org/dictionaries/mmcif_pdbx_v50.dic/Items/_pdbx_audit_revision_details.provider.html">
