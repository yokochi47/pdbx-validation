            <xsd:element name="overall_SU_R_free" minOccurs="0" maxOccurs="1" nillable="true" type="xsd:decimal">
              <xsd:annotation>
                <xsd:documentation xml:lang="en" source="http://mmcif.wwpdb.org/dictionaries/mmcif_pdbx_v50.dic/Items/_refine.overall_SU_R_free.html">
