      &lt;PDBxv:cellular_location xsi:nil="true" /&gt;
      &lt;PDBxv:ncbi_tax_id&gt;Escherichia coli&lt;/PDBxv:ncbi_tax_id&gt;
      &lt;PDBxv:organ xsi:nil="true" /&gt;
