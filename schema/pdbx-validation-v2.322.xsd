   &lt;PDBxv:pdbx_struct_legacy_oper_list id="2"&gt;
      &lt;PDBxv:matrix11&gt;0.247&lt;/PDBxv:matrix11&gt;
      &lt;PDBxv:matrix12&gt;0.935&lt;/PDBxv:matrix12&gt;
      &lt;PDBxv:matrix13&gt;0.256&lt;/PDBxv:matrix13&gt;
