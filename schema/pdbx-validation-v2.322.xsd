        <xsd:complexType>
          <xsd:all>
            <xsd:element name="diameter" minOccurs="0" maxOccurs="1" nillable="true">
              <xsd:annotation>
                <xsd:documentation xml:lang="en" source="http://mmcif.wwpdb.org/dictionaries/mmcif_pdbx_v50.dic/Items/_em_virus_shell.diameter.html">
The value of the diameter (in angstroms) for this virus shell.
524.0
                        </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:minExclusive value="0.0"/>
