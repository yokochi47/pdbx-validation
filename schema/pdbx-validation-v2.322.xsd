                        </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="R"/>
                  <xsd:enumeration value="S"/>
                  <xsd:enumeration value="N"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="atom_stereo_config_2" minOccurs="0" maxOccurs="1" nillable="true">
              <xsd:annotation>
                <xsd:documentation xml:lang="en" source="http://mmcif.wwpdb.org/dictionaries/mmcif_pdbx_v50.dic/Items/_pdbx_entity_poly_comp_link_list.atom_stereo_config_2.html">
