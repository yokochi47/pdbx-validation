      &lt;PDBxv:upper_limit&gt;2.70&lt;/PDBxv:upper_limit&gt;
   &lt;/PDBxv:pdbx_bond_distance_limits&gt;
   &lt;PDBxv:pdbx_bond_distance_limits atom_type_1="O" atom_type_2="Ag"&gt;
      &lt;PDBxv:lower_limit&gt;1.85&lt;/PDBxv:lower_limit&gt;
      &lt;PDBxv:upper_limit&gt;2.70&lt;/PDBxv:upper_limit&gt;
   &lt;/PDBxv:pdbx_bond_distance_limits&gt;
