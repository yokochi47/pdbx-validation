      &lt;PDBxv:name&gt;ANSIG&lt;/PDBxv:name&gt;
      &lt;PDBxv:version&gt;3.0&lt;/PDBxv:version&gt;
   &lt;/PDBxv:pdbx_nmr_software&gt;
   &lt;PDBxv:pdbx_nmr_software ordinal="4"&gt;
