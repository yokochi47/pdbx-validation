                        </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="parent_comp_id" minOccurs="0" maxOccurs="1" nillable="true" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation xml:lang="en" source="http://mmcif.wwpdb.org/dictionaries/mmcif_pdbx_v50.dic/Items/_pdbx_struct_mod_residue.parent_comp_id.html">
