          <xsd:attribute name="pdbx_refine_id" use="required" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en" source="http://mmcif.wwpdb.org/dictionaries/mmcif_pdbx_v50.dic/Items/_refine_funct_minimized.pdbx_refine_id.html">
