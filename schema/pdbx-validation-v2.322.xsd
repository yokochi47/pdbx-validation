            <xsd:element name="d_res_high" minOccurs="1" maxOccurs="1" nillable="true">
              <xsd:annotation>
                <xsd:documentation xml:lang="en" source="http://mmcif.wwpdb.org/dictionaries/mmcif_pdbx_v50.dic/Items/_reflns_shell.d_res_high.html">
