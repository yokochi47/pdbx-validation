      &lt;PDBxv:db_seq_id&gt;25&lt;/PDBxv:db_seq_id&gt;
      &lt;PDBxv:details xsi:nil="true" /&gt;
   &lt;/PDBxv:pdbx_struct_ref_seq_deletion&gt;
&lt;/PDBxv:pdbx_struct_ref_seq_deletionCategory&gt;


