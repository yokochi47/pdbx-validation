      &lt;PDBxv:ptnr1_label_seq_id&gt;87&lt;/PDBxv:ptnr1_label_seq_id&gt;
      &lt;PDBxv:ptnr1_role&gt;positive&lt;/PDBxv:ptnr1_role&gt;
