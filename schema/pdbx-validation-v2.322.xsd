                        </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="percent_possible_all" minOccurs="0" maxOccurs="1" nillable="true">
              <xsd:annotation>
                <xsd:documentation xml:lang="en" source="http://mmcif.wwpdb.org/dictionaries/mmcif_pdbx_v50.dic/Items/_reflns_shell.percent_possible_all.html">
