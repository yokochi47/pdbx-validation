      &lt;PDBxv:auth_asym_id&gt;A&lt;/PDBxv:auth_asym_id&gt;
      &lt;PDBxv:auth_seq_id&gt;105&lt;/PDBxv:auth_seq_id&gt;
      &lt;PDBxv:comp_id&gt;TRP&lt;/PDBxv:comp_id&gt;
