of attribute B_iso in category phasing_MIR_der_site. 
                        </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:extension base="xsd:decimal">
                    <xsd:attribute fixed="angstroms" name="units" type="xsd:string" use="optional"/>
                  </xsd:extension>
                </xsd:simpleContent>
