      &lt;PDBxv:ptnr2_role&gt;negative&lt;/PDBxv:ptnr2_role&gt;
      &lt;PDBxv:ptnr2_symmetry&gt;1_555&lt;/PDBxv:ptnr2_symmetry&gt;
   &lt;/PDBxv:struct_conn&gt;
   &lt;PDBxv:struct_conn id="C2"&gt;
      &lt;PDBxv:conn_type_id&gt;hydrog&lt;/PDBxv:conn_type_id&gt;
      &lt;PDBxv:details xsi:nil="true" /&gt;
