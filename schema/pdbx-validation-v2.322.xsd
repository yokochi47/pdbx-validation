      &lt;PDBxv:end_auth_seq_id&gt;525&lt;/PDBxv:end_auth_seq_id&gt;
   &lt;/PDBxv:pdbx_struct_nmr_ens_dom_lim&gt;
   &lt;PDBxv:pdbx_struct_nmr_ens_dom_lim component_id="2" dom_id="1"&gt;
      &lt;PDBxv:beg_auth_asym_id&gt;A&lt;/PDBxv:beg_auth_asym_id&gt;
      &lt;PDBxv:beg_auth_comp_id&gt;ASN&lt;/PDBxv:beg_auth_comp_id&gt;
      &lt;PDBxv:beg_auth_seq_id&gt;530&lt;/PDBxv:beg_auth_seq_id&gt;
      &lt;PDBxv:end_auth_asym_id&gt;A&lt;/PDBxv:end_auth_asym_id&gt;
      &lt;PDBxv:end_auth_comp_id&gt;LYS&lt;/PDBxv:end_auth_comp_id&gt;
      &lt;PDBxv:end_auth_seq_id&gt;555&lt;/PDBxv:end_auth_seq_id&gt;
   &lt;/PDBxv:pdbx_struct_nmr_ens_dom_lim&gt;
   &lt;PDBxv:pdbx_struct_nmr_ens_dom_lim component_id="3" dom_id="1"&gt;
      &lt;PDBxv:beg_auth_asym_id&gt;A&lt;/PDBxv:beg_auth_asym_id&gt;
      &lt;PDBxv:beg_auth_comp_id&gt;ASP&lt;/PDBxv:beg_auth_comp_id&gt;
      &lt;PDBxv:beg_auth_seq_id&gt;560&lt;/PDBxv:beg_auth_seq_id&gt;
      &lt;PDBxv:end_auth_asym_id&gt;A&lt;/PDBxv:end_auth_asym_id&gt;
      &lt;PDBxv:end_auth_comp_id&gt;ARG&lt;/PDBxv:end_auth_comp_id&gt;
      &lt;PDBxv:end_auth_seq_id&gt;616&lt;/PDBxv:end_auth_seq_id&gt;
   &lt;/PDBxv:pdbx_struct_nmr_ens_dom_lim&gt;
&lt;/PDBxv:pdbx_struct_nmr_ens_dom_limCategory&gt;


         </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="pdbx_struct_nmr_ens_dom_lim" minOccurs="0" maxOccurs="unbounded">
        <xsd:complexType>
