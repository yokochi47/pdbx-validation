      &lt;PDBxv:db_seq_id&gt;23&lt;/PDBxv:db_seq_id&gt;
      &lt;PDBxv:details xsi:nil="true" /&gt;
   &lt;/PDBxv:pdbx_struct_ref_seq_deletion&gt;
   &lt;PDBxv:pdbx_struct_ref_seq_deletion id="1"&gt;
      &lt;PDBxv:asym_id&gt;A&lt;/PDBxv:asym_id&gt;
      &lt;PDBxv:comp_id&gt;THR&lt;/PDBxv:comp_id&gt;
