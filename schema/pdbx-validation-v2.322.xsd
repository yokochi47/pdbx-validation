      &lt;PDBxv:name&gt;FELIX&lt;/PDBxv:name&gt;
      &lt;PDBxv:version&gt;1.1&lt;/PDBxv:version&gt;
   &lt;/PDBxv:pdbx_nmr_software&gt;
   &lt;PDBxv:pdbx_nmr_software ordinal="3"&gt;
      &lt;PDBxv:authors&gt;Kraulis&lt;/PDBxv:authors&gt;
