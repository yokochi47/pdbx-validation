                  <xsd:enumeration value="P 4 21 2"/>
                  <xsd:enumeration value="P 3"/>
                  <xsd:enumeration value="P 3 1 2"/>
                  <xsd:enumeration value="P 3 2 1"/>
                  <xsd:enumeration value="P 6"/>
                  <xsd:enumeration value="P 6 2 2"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
          <xsd:attribute name="id" use="required" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation xml:lang="en" source="http://mmcif.wwpdb.org/dictionaries/mmcif_pdbx_v50.dic/Items/_em_2d_crystal_entity.id.html">
